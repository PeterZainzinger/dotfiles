alias nvimrc='nvim ~/dotfiles/.nvimrc'

alias c='clear'
alias o='open'
alias gc='git commit'
alias gd='git diff'
alias ga='git add -A'

alias spotify="snap run spotify  --force-device-scale-factor=2.0 "
alias idea="/opt/jetbrains-toolbox"

alias display-home="xrandr --output eDP-1 --auto --right-of DP-1"
alias display-dock="xrandr --output eDP-1 --auto --right-of DP-1-2"

